  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "node src/server.js",
    "start-watch": "gulp watch",
    "seed": "node src/commands/seed.js",
    "import": "node src/commands/import.js",
    "users": "node src/commands/users.js",
    "pretty": "prettier --write",
    "pretty-all": "yarn pretty \"{src,__tests__}/**/*.js\"",
    "lint": "eslint",
    "lint-all": "yarn lint {src,__tests__}/**/*.js",
    "precommit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js"
    ]
  },
  "dependencies": {
    "@danilupion/server-utils": "^0.2.0",
    "bcrypt": "^3.0.6",
    "ect": "^0.5.9",
    "express": "^4.17.1",
    "gm": "^1.23.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "shortid": "^2.2.15",
    "uuid": "^3.3.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gulp": "4.0.2",
    "gulp-develop-server": "^0.5.2",
    "gulp-watch": "^5.0.1",
    "husky": "^3.0.7",
    "jest": "24.9.0",
    "lint-staged": "^9.4.1",
    "mockingoose": "^2.13.2",
    "prettier": "^1.18.2"
  }
}
