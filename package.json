  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "node src/server.js",
    "start-watch": "gulp watch",
    "seed": "node src/commands/seed.js",
    "import": "node src/commands/import.js",
    "pretty": "prettier --write",
    "pretty-all": "yarn pretty \"{src,__tests__}/**/*.js\"",
    "lint": "eslint",
    "lint-all": "yarn lint {src,__tests__}/**/*.js",
    "precommit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js"
    ]
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "express": "^4.16.3",
    "gm": "^1.23.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.4",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "shortid": "^2.2.8",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "gulp": "4.0.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-watch": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "23.1.0",
    "lint-staged": "^7.1.3",
    "mockingoose": "^2.10.1",
    "prettier": "^1.13.4"
  }
}
