{
  "name": "gifcept-server",
  "version": "0.1.0",
  "repository": "git@github.com:tomaccodev/gifcept-server.git",
  "author": {
    "name": "Dani Lupi√≥n",
    "email": "danilupion@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "node src/server.js",
    "seed": "node src/commands/seed.js",
    "pretty": "prettier --write",
    "pretty-all": "yarn pretty \"{src,__tests__}/**/*.js\"",
    "lint": "eslint",
    "lint-all": "yarn lint {src,__tests__}/**/*.js",
    "precommit": "lint-staged && yarn test"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js"
    ]
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "jest": "23.0.1",
    "lint-staged": "^7.1.2",
    "mockingoose": "^2.10.1",
    "prettier": "^1.12.1"
  }
}
