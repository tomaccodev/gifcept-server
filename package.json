    "@types/mongoose": "^5.7.14",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.3",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^13.13.4",
    "@types/passport": "^1.0.3",
